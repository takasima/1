<mtapp:setting
    id="editor_style_css"
    label="editor_style.css:">
    <input type="text" id="editor_style_css" name="editor_style_css" value="<mt:var name="editor_style_css" escape="html">" class="text full" />
</mtapp:setting>
<mtapp:setting
    id="editor_plugins"
    label="plugins:">
    <textarea id="editor_plugins" name="editor_plugins" class="text full low"><mt:var name="editor_plugins" escape="html"></textarea>
</mtapp:setting>
<mtapp:setting
    id="theme_advanced_buttons1"
    label="advanced_buttons1:">
    <textarea id="theme_advanced_buttons1" name="theme_advanced_buttons1" class="text full low"><mt:var name="theme_advanced_buttons1" escape="html"></textarea>
    <div class="hint"><__trans phrase="&quot;emoji&quot; is enabled when &quot;KeitaiLib plugin&quot; is used"></div>
</mtapp:setting>
