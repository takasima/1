<mtapp:setting
    id="editor_skin"
    label="<__trans phrase="Skin">">
    <input type="checkbox" id="editor_skin" name="editor_skin" value="1"<mt:if name="editor_skin"> checked="checked"</mt:if> style="vertical-align:middle" />
    <label for="editor_skin"><__trans phrase="Enable"></label>
</mtapp:setting>
<mt:ignore>
<mtapp:setting
    id="editor_toolbar_position"
    label="<__trans phrase="Toolbar position">">
    <li><input type="radio" name="editor_toolbar_position" value="1"<mt:if name="editor_toolbar_position" eq="1"> checked="checked"</mt:if> style="vertical-align:middle" /> <__trans phrase="Fix header"></li>
    <li><input type="radio" name="editor_toolbar_position" value="0"<mt:if name="editor_toolbar_position" eq="0"> checked="checked"</mt:if> style="vertical-align:middle" /> <__trans phrase="Default"></li>
    <p><__trans phrase="Internet Explorer 6 &quot;Fix header&quot; does not apply."></p>
</mtapp:setting>
</mt:ignore>
<mtapp:setting
    id="editor_advanced_setting"
    label="<__trans phrase="Advanced Setting">">
    <textarea id="editor_advanced_setting" name="editor_advanced_setting" class="text full high"><mt:var name="editor_advanced_setting" escape="html"></textarea>
    <div class="hint"><__trans phrase="e.g">) tinymce_editor.init['theme_advanced_blockformats'] = 'p,div,h1,h2,h3,h4';</div>
</mtapp:setting>
