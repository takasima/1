<mt:setvarblock name="system_msg" append="1">
    <div id="msg-container">
    <mt:if name="dynamic_all">
        <mt:setvar name="rebuild" value="">
    <mt:elseif name="request.no_rebuild">
        <mt:setvar name="rebuild" value="">
    <mt:else>
        <mt:if name="blog_id">
            <mt:setvar name="rebuild" value="all">
        <mt:else>
            <mt:setvar name="rebuild" value="">
        </mt:if>
    </mt:if>
    <mt:if name="request.saved">
        <mtapp:statusmsg
            id="saved"
            class="success"
            rebuild="$rebuild">
            <__trans phrase="Your changes have been saved.">
        </mtapp:statusmsg>
    </mt:if>
    <mt:if name="request.saved_deleted">
        <mtapp:statusmsg
            id="saved-deleted"
            class="success"
            rebuild="$rebuild">
        <__trans_section component="ContactForm">
        <__trans phrase="The Form Elements has been deleted from the database.">
        </__trans_section>
        </mtapp:statusmsg>
    </mt:if>
    </div>
</mt:setvarblock>
<mt:setvarblock name="html_head" append="1">
<__trans_section component="ContactForm">
<style type="text/css">.icon-none{ display:none }</style>
<script type="text/javascript">
var select_csv;
function file_select() {
    select_csv = 1;
    getByID( 'send_csv' ).style.display = 'inline';
}
function upload_csv() {
    if ( getByID( 'csv' ).style.display == 'none' ) {
        getByID( 'csv' ).style.display = 'inline';
        return false;
    }
    if ( select_csv == 1 ) {
        if ( window.confirm( '<__trans phrase="Are you sure you want to import Form from CSV?">' ) ) {
            var form = getByID( 'upload_contactform_csv' );
            form.submit();
        }
    } else {
        alert( '<__trans phrase="Please select a file to upload.">' );
    }
    return false;
}
</script>
</__trans_section>
</mt:setvarblock>