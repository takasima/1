/*============================================================**
#top
**============================================================*/

#contents {
    margin: 0 0 7px 0;
    padding: 0 0 0 0;
}

#contents section {
    margin: 0;
}

#contents nav{
    padding: 10px 0;
    background: #FFFFFF;
    -webkit-box-shadow: 0 10px 10px #BBB;
    margin-bottom: 15px;
}

#contents nav .nav1{
    margin: 0 7px;
}

#contents aside{
    width: 305px;
    margin: 0 auto 10px auto;
}

#contents aside li{
    list-style: none;
    display: inline;
    margin-bottom: 5px;
}

#contents aside li:nth-child(odd){
    margin-right: 9px;
    width: 145px;

}

#contents aside li img{
    margin-bottom: 7px;
}

#contents table{
    margin: 0 13px 13px;
    font-size: 12px;
}

#contents div.panel th{
    display: inline;
    line-height: 1.8;
}

#contents div.panel td{
    display: block;
    margin-bottom: 5px;
    padding-bottom: 5px;
    border-bottom: solid 1px #C9C9C9;
}

#contents div.panel tr:last-child td{
    border-bottom: none;
}
